# LLM Service Configuration
LLM_ENDPOINT=http://localhost:11434/v1/chat/completions
LLM_APIKEY=not-needed-for-local

# File Upload Configuration
UPLOAD_PATH=files

# Spring Profile
SPRING_PROFILES_ACTIVE=dev

# PostgreSQL Database Configuration (for docker-compose with PostgreSQL)
# IMPORTANT: Change these values for production deployments!
POSTGRES_DB=resume_db
POSTGRES_USER=resume_user
POSTGRES_PASSWORD=resume_password

# Database Connection (for Spring Boot when using PostgreSQL)
# Note: These are read by docker-compose, which will substitute the values above
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/resume_db
SPRING_DATASOURCE_USERNAME=resume_user
SPRING_DATASOURCE_PASSWORD=resume_password
